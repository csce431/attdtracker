<div class="page-content">

<h1>Class Roster</h1>
<table>
  <tr>
    <th>Student Name</th>
    <th>Card Number</th>
  </tr>
 
  <% @homes.each do |home| %>
    <tr>
      <td><%= link_to home.name, home %></td>
      <td><%= link_to home.card, edit_home_path(home) %></td>
      
      <!--
      <td><%= link_to 'Edit', edit_home_path(home) %></td>      
      <td><%= link_to 'Remove', home,
              method: :delete,
              data: { confirm: 'Are you sure?' } %></td>
      -->
    </tr>
  <% end %>
</table>
</div>
